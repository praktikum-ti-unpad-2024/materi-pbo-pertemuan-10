#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/Kuliah/AsPrak/materi/materi-pbo-pertemuan-10/node_modules/.pnpm/@slidev+cli@0.43.15_@babel+_f5c05ef6bffde28073dc8bb19dd2d25e/node_modules/@slidev/cli/bin/node_modules:/mnt/e/Kuliah/AsPrak/materi/materi-pbo-pertemuan-10/node_modules/.pnpm/@slidev+cli@0.43.15_@babel+_f5c05ef6bffde28073dc8bb19dd2d25e/node_modules/@slidev/cli/node_modules:/mnt/e/Kuliah/AsPrak/materi/materi-pbo-pertemuan-10/node_modules/.pnpm/@slidev+cli@0.43.15_@babel+_f5c05ef6bffde28073dc8bb19dd2d25e/node_modules/@slidev/node_modules:/mnt/e/Kuliah/AsPrak/materi/materi-pbo-pertemuan-10/node_modules/.pnpm/@slidev+cli@0.43.15_@babel+_f5c05ef6bffde28073dc8bb19dd2d25e/node_modules:/mnt/e/Kuliah/AsPrak/materi/materi-pbo-pertemuan-10/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/Kuliah/AsPrak/materi/materi-pbo-pertemuan-10/node_modules/.pnpm/@slidev+cli@0.43.15_@babel+_f5c05ef6bffde28073dc8bb19dd2d25e/node_modules/@slidev/cli/bin/node_modules:/mnt/e/Kuliah/AsPrak/materi/materi-pbo-pertemuan-10/node_modules/.pnpm/@slidev+cli@0.43.15_@babel+_f5c05ef6bffde28073dc8bb19dd2d25e/node_modules/@slidev/cli/node_modules:/mnt/e/Kuliah/AsPrak/materi/materi-pbo-pertemuan-10/node_modules/.pnpm/@slidev+cli@0.43.15_@babel+_f5c05ef6bffde28073dc8bb19dd2d25e/node_modules/@slidev/node_modules:/mnt/e/Kuliah/AsPrak/materi/materi-pbo-pertemuan-10/node_modules/.pnpm/@slidev+cli@0.43.15_@babel+_f5c05ef6bffde28073dc8bb19dd2d25e/node_modules:/mnt/e/Kuliah/AsPrak/materi/materi-pbo-pertemuan-10/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/slidev.mjs" "$@"
else
  exec node  "$basedir/../../bin/slidev.mjs" "$@"
fi
