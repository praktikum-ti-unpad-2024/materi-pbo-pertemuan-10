#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/Kuliah/AsPrak/materi/materi-pbo-pertemuan-10/node_modules/nypm/dist/node_modules:/mnt/e/Kuliah/AsPrak/materi/materi-pbo-pertemuan-10/node_modules/nypm/node_modules:/mnt/e/Kuliah/AsPrak/materi/materi-pbo-pertemuan-10/node_modules:/mnt/e/Kuliah/AsPrak/materi/node_modules:/mnt/e/Kuliah/AsPrak/node_modules:/mnt/e/Kuliah/node_modules:/mnt/e/node_modules:/mnt/e/Kuliah/AsPrak/materi/materi-pbo-pertemuan-10/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/Kuliah/AsPrak/materi/materi-pbo-pertemuan-10/node_modules/nypm/dist/node_modules:/mnt/e/Kuliah/AsPrak/materi/materi-pbo-pertemuan-10/node_modules/nypm/node_modules:/mnt/e/Kuliah/AsPrak/materi/materi-pbo-pertemuan-10/node_modules:/mnt/e/Kuliah/AsPrak/materi/node_modules:/mnt/e/Kuliah/AsPrak/node_modules:/mnt/e/Kuliah/node_modules:/mnt/e/node_modules:/mnt/e/Kuliah/AsPrak/materi/materi-pbo-pertemuan-10/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nypm/dist/cli.mjs" "$@"
else
  exec node  "$basedir/../nypm/dist/cli.mjs" "$@"
fi
